{
  "name": "gado-gado",
  "version": "1.0.0",
  "description": "themes gado2",
  "main": "index.js",
  "devDependencies": {
    "archiver": "^3.1.1",
    "autoprefixer": "^9.7.1",
    "browser-sync": "^2.26.7",
    "chalk": "^2.4.2",
    "clean-css-cli": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-wordpress": "^2.0.0",
    "node-sass": "^4.14.1",
    "node-wp-i18n": "^1.2.3",
    "npm-run-all": "^4.1.5",
    "onchange": "^3.2.1",
    "parallelshell": "github:darkguy2008/parallelshell#8fd83e2",
    "postcss-cli": "^6.1.3",
    "pretty-bytes": "^5.3.0",
    "recursive-readdir": "^2.2.2",
    "stylelint": "^9.10.1",
    "stylelint-config-wordpress": "^13.1.0"
  },
  "scripts": {
    "autoprefixer": "postcss assets/css/style.css --use autoprefixer --replace --no-map --verbose",
    "makepot": "node .scripts/makepot.js",
    "lint:css": "stylelint \"assets/css/style.css\" ",
    "lint:js": "eslint .",
    "fix:js": "eslint . --fix",
    "zip": "node .scripts/makezip.js",
    "scss": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/style.scss assets/css/style.css",
    "scss-gutenberg-back": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/scss/style-editor.scss lib/gutenberg/style-editor.css",
    "scss-gutenberg-front": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/scss/front-end.scss lib/gutenberg/front-end.css",
    "build:css": "npm run scss && npm run autoprefixer",
    "watch:css": "onchange \"scss/**/*.scss\" -- npm run build:css",
    "serve": "browser-sync start --proxy \"http://localhost/training_frontend/course_day_7/domigadov2_hsn\" --files \"assets/css/style.css\" \"*.php\" \"page-templates/*.php\" \"template-parts/*.php\" \"lib/**/*.php\" \"lib/gutenberg/*.css\" \"lib/woocommerce/*.css\" \"lib/amp/*.css\" \"scss/**/*.scss\" ",
    "watch:parallel": "parallelshell \"npm run serve\" \"npm run watch:css\"",
    "watch": "npm run watch:css",
    "css-minify": "cleancss --level 1 --format breaksWith=lf --source-map --source-map-inline-sources --output style.min.css style.css"
  },
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "last 1 Android versions",
    "last 1 ChromeAndroid versions",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Edge versions",
    "last 2 Opera versions"
  ],
  "author": "",
  "license": "ISC"
}
